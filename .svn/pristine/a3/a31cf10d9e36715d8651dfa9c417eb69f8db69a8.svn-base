#import <Foundation/Foundation.h>

@interface PersonalSettingData : NSObject
{
    BOOL m_hasSwitch;
    BOOL m_hasIndicator;
    BOOL m_hasImageView;
    BOOL m_hasLabel;
    BOOL m_switchState;
    BOOL m_hasSelected;
    BOOL m_hasHeaderLine;
    BOOL m_isOnLine;
    
    NSString * m_content;
    UIImage * m_image;
    NSString * m_title;
    CGFloat m_cellHeight;
    
    CGFloat m_textHeight;
    
    BOOL m_needChangeTitleFrame;
}

@property (nonatomic, assign) BOOL m_hasSwitch;
@property (nonatomic, assign) BOOL m_hasIndicator;
@property (nonatomic, assign) BOOL m_hasImageView;
@property (nonatomic, assign) BOOL m_hasLabel;
@property (nonatomic, assign) BOOL m_switchState;
@property (nonatomic, assign) BOOL m_hasSelected;
@property (nonatomic, assign) BOOL m_hasHeaderLine;
@property (nonatomic, assign) BOOL m_isOnLine;

@property (nonatomic, strong) NSString * m_content;
@property (nonatomic, strong) UIImage * m_image;
@property (nonatomic, strong) NSString * m_title;
@property (nonatomic, assign) CGFloat m_cellHeight;

@property (nonatomic, assign) CGFloat m_textHeight;
@property (nonatomic, assign) BOOL m_needChangeTitleFrame;



@end
